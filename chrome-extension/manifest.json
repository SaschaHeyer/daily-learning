{
    "manifest_version": 3,
    "name": "Send URL to API",
    "description": "Send the current tab's URL to an API when the extension icon is clicked.",
    "version": "1.0",
    "permissions": ["activeTab", "scripting"],
    "action": {
      "default_icon": {
        "16": "icon.png",
        "48": "icon.png",
        "128": "icon.png"
      }
    },
    "background": {
      "service_worker": "background.js"
    }
  }
  